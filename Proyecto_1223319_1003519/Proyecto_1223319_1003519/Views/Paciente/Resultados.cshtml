@model IEnumerable<Proyecto_1223319_1003519.Models.LlavePaciente>

@{
    ViewBag.Title = "Resultados";
}

<h2>Resultados de la busqueda</h2>

<label>Resultados: </label>@ViewBag.Resultados

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DPI)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Apellido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Edad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Estado)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DPI)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apellido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Edad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estado)
            </td>
            <td>
                @Html.ActionLink("Detalles", "Details", new { id = item.DPI })
            </td>
        </tr>
    }

</table>

<p>
    @Html.ActionLink("Volver al inicio", "Index", "Paciente")
</p>
